{
  "name": "eslint-plugin-beian",
  "version": "0.0.1",
  "description": "remind developers that the code needs to put on record",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "kkopite <kkopitehong@foxmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/action-hong/eslint-plugin-beian.git"
  },
  "main": "./dist/index.cjs",
  "exports": "./dist/index.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && unbuild",
    "stub": "unbuild --stub",
    "test": "vitest",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^6.4.0",
    "@typescript-eslint/utils": "^6.4.0",
    "add": "^2.0.6",
    "bumpp": "^9.1.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/rule-tester": "^6.4.0",
    "eslint": "^8.19.0",
    "eslint-doc-generator": "^1.0.0",
    "eslint-plugin-eslint-plugin": "^5.0.0",
    "eslint-plugin-node": "^11.1.0",
    "lint-staged": "^14.0.0",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1",
    "vitest": "^0.34.1"
  },
  "engines": {
    "node": "^14.17.0 || ^16.0.0 || >= 18.0.0"
  },
  "peerDependencies": {
    "eslint": ">=7"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "license": "ISC"
}
